{
  "name": "@react-docusaurus/theme-common",
  "version": "2.1.0",
  "description": "Common code for Docusaurus themes.",
  "repository": {
    "type": "git",
    "url": "https://github.com/hkngoc/react-docusaurus",
    "directory": "packages/docusaurus-theme-common"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "start": "tsc -p tsconfig.esm.json --watch",
    "storybook": "start-storybook -p 6006 --no-dll",
    "build-storybook": "build-storybook --no-dll"
  },
  "license": "MIT",
  "dependencies": {
    "@react-docusaurus/core": "2.1.0",
    "@docusaurus/types": "2.1.0",
    "@docusaurus/mdx-loader": "2.1.0",
    "@docusaurus/module-type-aliases": "2.1.0",
    "@docusaurus/plugin-content-blog": "2.1.0",
    "@docusaurus/plugin-content-docs": "2.1.0",
    "@docusaurus/plugin-content-pages": "2.1.0",
    "@docusaurus/utils": "2.1.0",
    "@types/history": "^4.7.11",
    "@types/react": "*",
    "@types/react-router-config": "*",
    "clsx": "^1.2.1",
    "parse-numeric-range": "^1.3.0",
    "prism-react-renderer": "^1.3.5",
    "tslib": "^2.4.0",
    "utility-types": "^3.10.0"
  },
  "devDependencies": {
    "fs-extra": "^10.1.0",
    "lodash": "^4.17.21"
  },
  "peerDependencies": {
    "react": "^16.8.4 || ^17.0.0",
    "react-dom": "^16.8.4 || ^17.0.0"
  },
  "engines": {
    "node": ">=16.14"
  }
}
